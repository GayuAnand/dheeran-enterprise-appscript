{
  "name": "cap-webview",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "build:ghpages": "ng build --configuration production --deploy-url dheeran-enterprise-appscript/",
    "inc:buildnumber": "node bin/increment-apk-buildnumber.js",
    "pipeline:apk": "npm run inc:buildnumber && npm run build:prod && node bin/build-zip.js && npm run sync && npm run android:build",
    "pipeline:ghpages": "npm run pipeline:apk && npm run build:ghpages && node bin/copy-zip.js && node bin/populate-dist-apks.js && node bin/publish-ghpages.js",
    "watch": "ng build --watch --configuration development",
    "android:add": "npx cap add android",
    "android:run": "npx cap run android",
    "android:open": "npx cap open android",
    "android:build": "cd android && ./gradlew assembleDebug && cd ..",
    "sync": "npx cap sync",
    "reset:ghpages": "rm -rf node_modules/.cache/gh-pages && git push origin :gh-pages",
    "generate:logos": "npx @capacitor/assets generate --assetPath 'logos' --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.5",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.5",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@capacitor/android": "^5.4.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.4.1",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/network": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capgo/capacitor-updater": "^5.3.34",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular/cli": "~16.2.3",
    "@angular/compiler-cli": "^16.2.0",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.4.1",
    "@types/jasmine": "~4.3.0",
    "archiver": "^6.0.1",
    "fs": "^0.0.1-security",
    "gh-pages": "^6.1.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
