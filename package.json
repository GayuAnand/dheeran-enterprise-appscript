{
  "name": "cap-webview",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8200",
    "start:server:dev": "nodemon server/app.js",
    "start:server:prod": "cd server && forever start app.js",
    "start:apiserver:debug": "cd server && nodemon --inspect app.js",
    "awslogin": "ssh -i 'server/AWS Web Server EC2 Instance.pem' ubuntu@51.20.95.178",
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "inc:buildnumber": "node bin/increment-apk-buildnumber.js",
    "pipeline:apk": "npm run inc:buildnumber && npm run build:prod && node bin/build-zip.js && npm run android:build:prod",
    "pipeline:amplify": "npm run pipeline:apk && node bin/copy-zip.js && node bin/populate-dist-apks.js && node bin/publish-ghpages.js",
    "build:publish:commit:sync": "npm run pipeline:amplify && git add . && git commit -s -m 'Update' && git push && git checkout gh-pages && git pull && git push ent gh-pages && git checkout main && git fetch origin --tags && git push ent --tags",
    "watch": "ng build --watch --configuration development",
    "android:add": "npx cap add android",
    "android:run": "npm run sync:dev && npx cap run android",
    "android:open": "npm run sync:dev && npx cap open android",
    "android:build:dev": "npm run sync:dev && cd android && ./gradlew assembleDebug && cd ..",
    "android:build:prod": "npm run sync:prod && cd android && ./gradlew assembleDebug && cd ..",
    "sync:dev": "cp capacitor.config/dev.ts ./capacitor.config.ts && npx cap sync",
    "sync:prod": "cp capacitor.config/prod.ts ./capacitor.config.ts && npx cap sync",
    "sha256:apk": "keytool -printcert -jarfile",
    "reset:ghpages": "rm -rf node_modules/.cache/gh-pages && git push origin :gh-pages",
    "generate:logos": "npx @capacitor/assets generate --assetPath 'logos' --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.5",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.5",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@asymmetrik/ngx-leaflet": "^16.0.1",
    "@asymmetrik/ngx-leaflet-markercluster": "^16.0.0",
    "@canvasjs/angular-charts": "^1.0.3",
    "@capacitor/android": "^5.4.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.4.1",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capgo/capacitor-updater": "^5.3.34",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "body-parser": "^1.20.2",
    "capacitor-plugin-incoming-call": "^0.0.9",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "jsdom": "^23.2.0",
    "leaflet": "^1.9.4",
    "leaflet.awesome-markers": "^2.0.5",
    "leaflet.markercluster": "^1.5.3",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.2",
    "normalize.css": "^8.0.1",
    "npm": "^10.2.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript-debounce-decorator": "^0.0.18",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular/cli": "~16.2.3",
    "@angular/compiler-cli": "^16.2.0",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.4.1",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.8",
    "@types/leaflet.awesome-markers": "^2.0.28",
    "@types/leaflet.markercluster": "^1.5.4",
    "archiver": "^6.0.1",
    "fs": "^0.0.1-security",
    "gh-pages": "^6.1.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.3",
    "typescript": "~5.1.3"
  }
}
